{
	"name": "directus-extension-interfaces",
	"description": "Collection of useful interfaces for Directus",
	"repository": {
		"type": "git",
		"url": "https://github.com/codihaus/Directus-Extension-Fieldplus.git"
	},
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "interface",
				"name": "advanced-filters",
				"source": "src/interface-advanced-filters/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.0.0",
		"typescript": "^5.5.4",
		"vue": "^3.4.37"
	}
}
